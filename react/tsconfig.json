{
  "compilerOptions": {
    "types": ["node", "grecaptcha"],
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "lib": ["es2017", "dom", "esnext"],
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2017"
  },
  "typeAcquisition": {
    "enable": false
  },
  "include": ["./**/*"],
  "exclude": ["node_modules"]
}
